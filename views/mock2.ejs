<!-- Landing page for "Wyldgreens" -->

<%- include("../views/partials/header.ejs") -%>

<div class="greeting-pane--home">
    <div class="fullwidth-wrapper">
        <div class="fullwidth-right-cont">
            <img src="/images/logo/Asset 2.png">
            <!-- <img class="grey-drop" src="/images/logo/grey-drop.png"> -->
        </div>
    </div>
</div>
<div class="lightbox">
    <img src="/images/greens/alek-cut.png">
</div>
<div class="l-body-wrapper--contact">
    <div class="l-content-wrapper--contact">

        <!-- HEADER -->

        <div class="how-it-works-wrapper">
            <h3>Delicious Microgreens<br>Delivered to Your Door</h3>
        </div>
        <div class="cta-back">
            <img class="green-arrow" src="/images/icons/cta/arrow.png">
            <img class="green-arrow second-arrow" src="/images/icons/cta/arrow.png">


            <div class="l-box-third">
                <div id="plan-button" class="cta-button-background make-smaller">
                    <!-- <p class="cta-num-1">1<span class="cta-num-colon">:</span></p> -->
                    <img class="cta-img" style="height: 118px; " src="/images/icons/cta/plan-leaf.png">
                    <h3>Pick Your Plan</h3>
                </div>
            </div>

            <div class="l-box-third">
                <div id="mix-button" class="cta-button-background make-smaller">
                    <!-- <p class="cta-num-2">2<span class="cta-num-colon">:</span></p> -->
                    <img class="cta-img" style="height: 116px; " src="/images/icons/cta/hand.png">
                    <h3>Pick Your Mix</h3>
                </div>
            </div>

            <div class="l-box-third">
                <div id="order-button" class="cta-button-background make-smaller">
                    <!-- <p class="cta-num-3">3<span class="cta-num-colon adjust-colon-down">:</span></p> -->
                    <img class="cta-img" style="top: 7px; height: 102px;  " src="/images/icons/cta/email-leaf.png">
                    <h3>Place An Order</h3>
                </div>
            </div>

        </div>

        <!-- WHY GO WYLD FARM SCENE -->

        <div class="farm-scene">
            <img class="cloud" src="/images/cloud.png">
            <h2 class="why-header">Why Go Wyld?</h2>
            <p class="committed">We are committed to serving our community by providing safe,
                locally sourced, chemical-free, nutrient-dense microgreens right to your door!</p>

            <!-- WHY POINTS -->

            <div class="wyld-why">

                <ul class="why-list">
                    <h3 class="why-subheader">A Healthier Planet - A Healthier You</h3>
                    <img class="list-item" style="width:128px; top: 8px; left: 2px; "
                        src="/images/icons/why/mountains.png">
                    <li class="mountains">Our Greens are grown locally in the Colorado foothills, free
                        from bugs, mold, and without the need for any harmful pesticides or chemicals
                    </li>
                    <br>
                    <img class="list-item" style="top: -12px; " src="/images/icons/why/custom.png">
                    <li class="custom">All of our different mixes are customizable, packed with flavor,
                        and dense in key nutrients, vitamins, flavonoids, and fiber</li>
                    <br>
                    <img class="list-item" style="top: -8px; " src="/images/icons/why/certified.png">
                    <li class="certified">We strive to deliver high-quality products with minimal waste
                        or footprint. Our packaging is reusable, recyclable, and keeps greens fresh for
                        weeks!</li>
                    <br>
                    <img class="list-item" style="top: 10px; " src="/images/icons/why/truck.png">
                    <li class="truck">We will personally deliver to your doorstep on your scheduled date
                        and pick up your used container - all free of charge with no contact necessary
                    </li>
                </ul>
            </div>
            <div style="height: 123px;"></div>

        </div>
        <div class="l-divider divider-bottom"></div>


        <!-- PLAN SECTION -->

        <div class="l-divider divider-top"></div>
        <div id="pick-plan" style="top: -70px; " class="avatar-pane--home adjust-up">
            <img class="section-img" style="width: 110px;" src="/images/icons/cta/plan-leaf-white.png">
            <h2>Pick a Plan</h2>
        </div>

        <!-- PLAN TEXT -->

        <div style="margin-top: -20px;" class="plan-text">
            <p>Each order comes in a reusable container. One box of greens is enough for 2 people to use
                as toppings or one person to use as a salad mix for one week. Multiple boxes can be
                ordered each week. Prices below are for one box of greens.</p>


        </div>

        <div style="height: 40px;"></div>
        <!-- PLAN ROW -->

        <div class="card-pane--tech">
            <div class="card card--tech-sides">
                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section side-plans">
                        <h3 class="plan-title">$12</h3>
                        <h3>One-time<br>Order</h3>

                    </div>

                    <div class="tech-card-bottom-section">
                        <div class="card-box-center">
                            <img class="leaf-bullet-top" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Perfect addition to<br>a dinner date</h4>
                            <img class="leaf-bullet-single" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Just treat yourself</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Give healthy<br>nutrition as a gift</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Add an extra to<br>your weekly order</h4>
                        </div>

                        <div id="one_time-pane" class="plan-btn-pane"></div>
                        <button id="one_time-order-btn" class="plan-order" type="button" onclick="oneTimeDel()">Order
                            Once</button>
                        <div id="one_time-added" class="plan-added">
                            <p>Added</p>
                            <img class="one_time-btn-check" src="/images/icons/check.png">
                        </div>
                    </div>

                </div>
            </div>

            <div class="card card--tech-center">
                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section center-plan">
                        <h3 class="plan-title">$10</h3>
                        <h3>Weekly Subscription</h3>
                        <h4 class="best-value">*Best Value!</h4>
                    </div>

                    <div class="tech-card-bottom-section">
                        <div class="card-box-center">
                            <img class="leaf-bullet-top" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Billed monthly, just<br>set it and forget it!</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Reusable container<br>picked up each week</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Always have fresh greens<br>for your family</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Stay green throughout<br>the cold winter!</h4>
                        </div>

                        <div id="weekly-pane" class="plan-btn-pane"></div>
                        <button id="weekly-order-btn" class="plan-order center-plan-btn" type="button"
                            onclick="weeklyDel()">Get Started</button>
                        <div id="weekly-added" class="plan-added">
                            <p>Added</p>
                            <img class="weekly-btn-check" src="/images/icons/check.png">
                        </div>
                    </div>

                </div>
            </div>

            <div class="card card--tech-sides">
                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section side-plans">
                        <h3 class="plan-title">$35</h3>
                        <h3>Bulk/Party Order</h3>
                    </div>

                    <div class="tech-card-bottom-section">
                        <div class="card-box-center">
                            <img class=" leaf-bullet-top" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">A full tray of greens,<br>3 boxes worth</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Great for restaurants<br>and special events</h4>
                            <img class="leaf-bullet-double" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Also available as a subscription</h4>
                            <img class="leaf-bullet-single" src="/images/icons/leaf.png">
                            <h4 class="plan-lower-items">Great for big parties!</h4>
                        </div>

                        <div id="bulk-pane" class="plan-btn-pane"></div>
                        <button id="bulk-order-btn" class="plan-order" type="button" onclick="bulkDel()">Stock
                            Up</button>
                        <div id="bulk-added" class="plan-added">
                            <p>Added</p>
                            <img class="bulk-btn-check" src="/images/icons/check.png">
                        </div>
                    </div>

                </div>
            </div>

        </div>

        <div style="height: 150px;"></div>

        <!-- PLAN TEXT -->


        <div style="margin-bottom: 30px;" class="plan-text">
            <p>Please pick a plan above to add it to your order.</p>
            <p>Schedule your order once a week, or setup a monthly payment and save! Order greens for
                someone you care about. Give the gift of nutrition! We have a reuseable container option
                to keep both your and our environmental footprint as low as possible. Show up to a
                dinner party with fresh food for the table. Bring the life to the party.</p>
        </div>

        <div class="l-divider divider-bottom"></div>


        <!-- PICK A MIX SECTION -->
        <!-- <div class="mix-back"></div> -->

        <div id="pick-mix" style="top: -15px;" class="avatar-pane--home">
            <img class="section-img" src="/images/icons/cta/hand-white.png">
            <h2>Pick a Mix</h2>
        </div>

        <div class="mix-body-text">
            <p>We offer three standard mixes, and a custom option. Click on an image below to add one to the order form.
            </p>
            <p>Please allow up to three weeks for special orders, depending on the status of our current crops.</p>
        </div>

        <div style="height: 140px;"></div>

        <!-- FIRST PRODUCT ROW -->

        <div class="card-pane--tech">

            <div class="card card--tech-left">
                <div class="remove-btn-cover"></div>
                <button class="farmers-remove" type="button" onclick="farmersCloseBtn()">Close</button>
                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section">
                        <h3>Farmer's Mix</h3>
                        <h4 class="mix-lower-heading">Crop dependent, let the farmers decide</h4>
                    </div>

                    <div class="tech-card-bottom-section">
                        <div class="">
                            <img onclick="runLightbox('green4.jpg')" class="card-art" src="/images/greens/green4.jpg">
                        </div>
                        <p class="mix-text">Let us pick your greens, we will make sure you get a tasty
                            combination.</p>

                        <div id="farmers-pane" class="mix-btn-pane"></div>
                        <button id="farmers-order-btn" class="mix-order-btn" type="button" onclick="farmersMix()">Add To
                            Order</button>
                        <div id="farmers-quantity" class="mix-quantity">
                            <img class="farmers-btn-check" src="/images/icons/check.png">
                            <img class="farmers-btn-cross" src="/images/icons/cross.png">

                            <p>How<br>Many?</p>
                            <input oninput="farmersUpdate()" class="mix-num-input" type="number" min="1" max="3"
                                value="1">
                        </div>

                    </div>

                </div>
            </div>

            <div class="card card--tech-right">
                <div class="remove-btn-cover"></div>
                <button class="spicy-remove" type="button" onclick="spicyCloseBtn()">Close</button>
                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section">
                        <h3>Spicy Mix</h3>
                        <h4 class="mix-lower-heading">Mustard, Radish Sprouts, and Sunflower</h4>
                    </div>

                    <div class="tech-card-bottom-section">
                        <div class="">
                            <img onclick="runLightbox('green2.jpg')" class="card-art" src="/images/greens/green2.jpg">
                        </div>
                        <p class="mix-text">Our Spicy Mix packs a punch for soups and salads, adding
                            robust flavors and zest.</p>

                        <div id="spicy-pane" class="mix-btn-pane"></div>
                        <button id="spicy-order-btn" class="mix-order-btn" type="button" onclick="spicyMix()">Add To
                            Order</button>
                        <div id="spicy-quantity" class="mix-quantity">
                            <img class="spicy-btn-check" src="/images/icons/check.png">
                            <img class="spicy-btn-cross" src="/images/icons/cross.png">

                            <p>How<br>Many?</p>
                            <input oninput="spicyUpdate()" class="mix-num-input" type="number" min="1" max="3"
                                value="1">
                        </div>
                    </div>

                </div>

            </div>

        </div>

        <div style="height: 260px;"></div>

        <!-- SECOND PRODUCT ROW -->

        <div class="card-pane--tech">
            <div class="card card--tech-left">
                <div class="remove-btn-cover"></div>
                <button class="salad-remove" type="button" onclick="saladCloseBtn()">Close</button>

                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section">
                        <h3>Salad Mix</h3>
                        <h4 class="mix-lower-heading">Sunflower, Broccoli, Cabbage, and Rutabaga</h4>
                    </div>

                    <div class="tech-card-bottom-section">
                        <div>
                            <img onclick="runLightbox('green1.jpg')" class="card-art" src="/images/greens/green1.jpg">
                        </div>
                        <p class="mix-text">The perfect mix for an all around healthy and tasty salad
                            year-round.</p>

                        <div id="salad-pane" class="mix-btn-pane"></div>
                        <button id="salad-order-btn" class="mix-order-btn" type="button" onclick="saladMix()">Add To
                            Order</button>
                        <div id="salad-quantity" class="mix-quantity">
                            <img class="salad-btn-check" src="/images/icons/check.png">
                            <img class="salad-btn-cross" src="/images/icons/cross.png">

                            <p>How<br>Many?</p>
                            <input oninput="saladUpdate()" class="mix-num-input" type="number" min="1" max="3"
                                value="1">
                        </div>
                    </div>

                </div>
            </div>

            <div class="card card--tech-right">
                <div class="remove-btn-cover"></div>
                <button class="healthy-remove" type="button" onclick="healthyCloseBtn()">Close</button>
                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section">
                        <h3>Healthy Mix</h3>
                        <h4 class="mix-lower-heading">Broccoli, Pea Sprouts,<br>and Radish</h4>
                    </div>

                    <div class="tech-card-bottom-section">
                        <div>
                            <img onclick="runLightbox('green2.jpg')" class="card-art" src="/images/greens/green2.jpg">
                        </div>
                        <p class="mix-text">Our Healthy Mix is a powerhouse of nutrients designed to
                            boost healing and recovery time.</p>

                        <div id="healthy-pane" class="mix-btn-pane"></div>

                        <button id="healthy-order-btn" class="mix-order-btn" type="button" onclick="healthyMix()">Add To
                            Order</button>
                        <div id="healthy-quantity" class="mix-quantity">
                            <img class="healthy-btn-check" src="/images/icons/check.png">
                            <img class="healthy-btn-cross" src="/images/icons/cross.png">

                            <p>How<br>Many?</p>
                            <input class="mix-num-input" type="number" min="1" max="3" value="1"
                                oninput="healthyUpdate()">
                        </div>

                    </div>

                </div>

            </div>

        </div>
        <div style="height: 260px;"></div>

        <!-- CUSTOM PRODUCT ROW -->

        <div class="card-pane--tech">

            <div class="card card--tech-right custom-order">

                <div class="tech-card-inner-wrap">

                    <div class="tech-card-top-section ">
                        <h3>Custom Order</h3>
                        <h4 class="mix-lower-heading">Pick one to four greens and build your own mix
                        </h4>
                    </div>

                    <div class="tech-card-bottom-section ">
                        <div class="wyld-left-section">
                            <img class="custom-img" src="/images/greens/top-down-transparent.png">
                        </div>
                        <div class="wyld-right-section">
                            <p class="custom-text">
                                Our premium, freshly grown ingredients are harvested and delivered the same day. Custom
                                orders may take a few weeks to initially fill.
                            </p>
                            <div class="ingred-wrap">

                                <button class="custom-new" type="button" onclick="customNewOrder()">Keep
                                    Shopping</button>
                                <button class="custom-close" type="button" onclick="customOrderOff()">Close</button>
                                <!-- <button class="custom-remove" type="button" onclick="customRemoveOrder()">Remove Order</button> -->

                                <div id="ingred-box" class="ingred-box">

                                    <ul class="custom-ingredients">
                                        <label for=" Broccoli"><input id=" Broccoli" class="custom-check"
                                                type="checkbox" disabled="true" name=" Broccoli"
                                                oninput="ingredCheck()">- Broccoli</label><br>
                                        <label for=" Sunflower"><input id=" Sunflower" class="custom-check"
                                                type="checkbox" disabled="true" name=" Sunflower"
                                                oninput="ingredCheck()">- Sunflower</label><br>
                                        <label for=" Rutabaga"><input id=" Rutabaga" class="custom-check"
                                                type="checkbox" disabled="true" name=" Rutabaga"
                                                oninput="ingredCheck()">- Rutabaga</label><br>
                                        <label for=" Pea"><input id=" Pea" class="custom-check" type="checkbox"
                                                disabled="true" name=" Pea" oninput="ingredCheck()">- Pea</label><br>
                                    </ul>
                                    <ul class="custom-ingredients">
                                        <label for=" Mustard"><input id=" Mustard" class="custom-check" type="checkbox"
                                                disabled="true" name=" Mustard" oninput="ingredCheck()">-
                                            Mustard</label><br>
                                        <label for=" Radish"><input id=" Radish" class="custom-check" type="checkbox"
                                                disabled="true" name=" Radish" oninput="ingredCheck()">-
                                            Radish</label><br>
                                        <label for=" Cabbage"><input id=" Cabbage" class="custom-check" type="checkbox"
                                                disabled="true" name=" Cabbage" oninput="ingredCheck()">-
                                            Cabbage</label><br>
                                    </ul>
                                    <div class="custom-btn-slider">

                                        <button class="custom-order-button" type="button"
                                            onclick="customOrderOn()">Start A New Custom Order</button>
                                        <button class="custom-order-hidden" type="button"
                                            onclick="promptQuantity()">Create
                                            Mix!</button>
                                        <div class="custom-quantity">
                                            <img class="custom-btn-check" src="/images/icons/check.png">
                                            <img class="custom-btn-cross" src="/images/icons/cross.png">

                                            <p>How<br>Many?</p>
                                            <input oninput="customUpdate()" class="custom-num-input" type="number"
                                                min="1" max="3" value="1">
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>
        <div style="height: 200px;"></div>
        <div class="l-divider divider-top"></div>


        <!-- ORDER FORM SECTION -->

        <div id="order-form" style="top: -43px;" class="avatar-pane--home adjust-order">
            <img class="section-img" src="/images/icons/cta/email-leaf-white.png">
            <h2>Place Your Order</h2>
        </div>

        <div class="order-text">
            <p>Review your order below. Use this form to make any additions or changes.
                We will be in contact shortly to confirm your order and send a payment link. Thanks!</p>
        </div>

        <div style="height: 20px;"></div>

        <!-- ORDER FORM -->

        <div class="card card--contact-form">
            <div class="contact-card-inner-wrap">

                <!-- HIDDEN RESPONSE -->

                <% if ( responseBool == true ) { %>

                <div id="hidden-response" class="hidden-response">
                    <div class="response-box">
                        <% if ( isError == true ) { %>
                        <p>Ah bummer!</p>
                        <p>Looks like something went wrong.</p>
                        <p>I'm currently looking into the problem, try back soon!</p>
                        <button type="button" class="hidden-button" onclick="location.href='/blog'" />Check out the
                        Blog</button>
                        <% } else { %>
                        <p>Thanks!</p>
                        <p>Your message has been sent</p>
                        <p>I'm looking forward to talking with you, I will reach out within 2 or 3 business days.</p>
                        <% } %>
                    </div>
                </div>

                <% } else { %>

                <!-- DISPLAY FORM -->

                <form id="contact-form" class="contact-form" name="contact" method="post">

                    <div class="contact-card-top-section">
                        <div class="wyld-contact-info">

                            <label class="form-title">Your Name:</label>
                            <input class="form-entry" type="text" name="user_name" placeholder="Jean Luc"
                                oninput="formFieldCheck()">
                            <label class="form-title">Email:</label>
                            <input class="form-entry" type="email" name="user_email" placeholder="example@email.com"
                                oninput="formFieldCheck()">
                            <label class="form-title">City/Town</label>
                            <input class="form-entry" type="text" name="user_city"
                                placeholder="Boulder, Superior, Louisville, Lafayette">
                            <label class="form-title">Phone Number</label>
                            <input class="form-entry" type="text" name="user_phone" placeholder="303 - 494 - 1274">
                            <button id="contact-button" class="contact-submit" type="submit"
                                onclick="sendingText()">Review<br>Your Order</button>
                        </div>
                        <div class="wyld-options">
                            <div class="wyld-form-plans">
                                <label for="one_time" class="form-title">One-time Order</label>
                                <input id="one_time" type="checkbox" name="one_time" oninput="oneTimeDel()">
                                <br>
                                <label for="weekly" class="form-title">Weekly Delivery</label>
                                <input id="weekly" type="checkbox" name="weekly" oninput="weeklyDel()">
                                <br>
                                <label for="bulk" class="form-title">Bulk Order</label>
                                <input id="bulk" type="checkbox" name="bulk" oninput="bulkDel()">
                                <p class="delivery-text">Deliveries always take place on<br>Thursday afternoons and
                                    evenings.</p>
                            </div>

                        </div>
                    </div>

                    <div class="contact-card-bottom-section">
                        <div class="wyld-form-mix">
                            <div class="mix-wrap">
                                <input type="checkbox" name="mix_farmers" oninput="farmersCheckbox()">
                                <label class="form-title mix-title">Farmer's Mix</label>
                                <input class="form-num" type="number" name="farmers-num" placeholder="0" min="0" max="3"
                                    onchange="farmersInput()">
                            </div>

                            <div class="mix-wrap">
                                <input type="checkbox" name="mix_spicy" oninput="spicyCheckbox()">
                                <label class="form-title mix-title">Spicy Mix</label>
                                <input class="form-num" type="number" name="spicy-num" placeholder="0" min="0" max="3"
                                    onchange="spicyInput()">
                            </div>

                            <div class="mix-wrap">
                                <input type="checkbox" name="mix_healthy" oninput="healthyCheckbox()">
                                <label class="form-title mix-title">Healthy Mix</label>
                                <input class="form-num" type="number" name="healthy-num" placeholder="0" min="0" max="3"
                                    onchange="healthyInput()">
                            </div>

                            <div class="mix-wrap">
                                <input type="checkbox" name="mix_salad" oninput="saladCheckbox()">
                                <label class="form-title mix-title">Salad Mix</label>
                                <input class="form-num" type="number" name="salad-num" placeholder="0" min="0" max="3"
                                    onchange="saladInput()">
                            </div>

                            <div class="mix-wrap">
                                <input type="checkbox" name="mix_custom">
                                <label class="form-title mix-title">Custom Mix</label>
                                <input class="form-num" type="number" name="custom-num" placeholder="0" min="0" max="3"
                                    onchange="customQuantityVerify()">
                            </div>
                        </div>

                        <div class="price-total">

                            <div class="mix-wrap">
                                <label class="form-title mix-title">Total</label>
                                <input class="price-num" type="number" name="total-num" placeholder="0">
                            </div>

                            <div class="bulk-discount-pane">
                                <p>Discount<br>Applied!</p>
                            </div>
                        </div>

                        <div class="form-text-wrap">
                            <p class="form-text-p">
                                Please leave us information regarding your order. We will be in
                                touch shortly, but would like to know if you have any special requests.
                            </p>
                            <p class="form-text-p">
