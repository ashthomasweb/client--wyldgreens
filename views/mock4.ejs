<div class="card card--contact-form">
    <div class="contact-card-inner-wrap">

        <% if ( responseBool == true ) { %>

        <div id="hidden-response" class="hidden-response">
            <div class="response-box">
                <% if ( isError == true ) { %>
                <p>Ah bummer!</p>
                <p>Looks like something went wrong.</p>
                <p>I'm currently looking into the problem, try back soon!</p>
                <button type="button" class="hidden-button" onclick="location.href='/blog'" />Check out the
                Blog</button>
                <% } else { %>
                <p>Thanks!</p>
                <p>Your message has been sent</p>
                <p>I'm looking forward to talking with you, I will reach out within 2 or 3 business days.</p>
                <!-- <button type="button" class="hidden-button" onclick="location.href='/blog'" />Check out the
                Blog</button> -->
                <% } %>
            </div>
        </div>

        <% } else { %>

        <form id="contact-form" class="contact-form" name="contact" method="post">

            <div class="contact-card-top-section">
                <div class="wyld-contact-info">

                    <label class="form-title">Your Name:</label>
                    <input class="form-entry" type="text" name="user_name" oninput="formFieldCheck()">
                    <label class="form-title">Email:</label>
                    <input class="form-entry" type="email" name="user_email" oninput="formFieldCheck()">
                    <label class="form-title">Address</label>
                    <input class="form-entry" type="text" name="user_address">
                    <label class="form-title">Phone Number</label>
                    <input class="form-entry" type="text" name="user_phone">
                    <button id="contact-button" class="contact-submit" type="submit"
                        onclick="sendingText()">Send</button>
                </div>
                <div class="wyld-options">

                    <div class="wyld-form-plans">
                        <label style="display: inline-block;" class="form-title">One-time</label>
                        <input style="display: inline-block;" type="checkbox" name="one_time">
                        <br>
                        <label style="display: inline-block;" class="form-title">Weekly Delivery</label>
                        <input style="display: inline-block;" type="checkbox" name="weekly">
                    </div>

                    <div class="wyld-form-mix">
                        <label style="display: inline-block;" class="form-title">Farmer's Mix</label>
                        <input style="display: inline-block;" type="checkbox" name="mix_farmers">
                        <br>
                        <label style="display: inline-block;" class="form-title">Custom Order</label>
                        <input style="display: inline-block;" type="checkbox" name="mix_custom">
                        <br>
                        <label style="display: inline-block;" class="form-title">Healthy Mix</label>
                        <input style="display: inline-block;" type="checkbox" name="mix_healthy">
                        <br>
                        <label style="display: inline-block;" class="form-title">Salad Mix</label>
                        <input style="display: inline-block;" type="checkbox" name="mix_salad">
                        <br>
                        <label style="display: inline-block;" class="form-title">Spicy Mix</label>
                        <input style="display: inline-block;" type="checkbox" name="mix_spicy">
                    </div>

                </div>
            </div>

            <div class="contact-card-bottom-section">
                <label class="form-title">Message:</label>
                <textarea class="form-textarea" name="message" oninput="formFieldCheck()"></textarea>
            </div>

        </form>

        <% } %>

    </div>
</div>
